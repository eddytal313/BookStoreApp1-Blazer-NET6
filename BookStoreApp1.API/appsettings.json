{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Host.Lifetime": "Information"
      }
    },
    "WriteTo": [

      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollinginterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {"serverUrl": "http://localhost:5341"}
      }
    ]
  },
  "AllowedHosts": "*"
}
